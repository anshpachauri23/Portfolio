PROGRAM BugTerminator IS

BEGIN
    WHILE true DO
        IF next-is-not-friend THEN
            infect
        ELSE
            IF next-is-wall THEN
                turnright              
            ELSE
                IF next-is-friend THEN
                    turnleft
                ELSE
                    move
                END IF
            END IF
        END IF
    END WHILE
END BugTerminator
                    
